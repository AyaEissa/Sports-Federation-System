        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SFS"
        mc:Ignorable="d"
        Title="Window1" Height="350" Width="500">
    <Grid Margin="1,2,-1.4,-1.6">
        <Grid.Background>
            <ImageBrush ImageSource="backgrd.png"/>
        </Grid.Background>
        <ListView x:Name="listView" HorizontalAlignment="Left" Height="123" Margin="77,114,0,0" VerticalAlignment="Top" Width="100">
            <ListView.View>
                <GridView>
                    <GridViewColumn/>
                </GridView>
            </ListView.View>
            <ListBox x:Name="listBox" Height="100" Width="100"/>
        </ListView>

    </Grid>
</Window>
